Jogo:
- ID -> Integer
- Nome -> String
- Data_Lancamento -> LocalDate
- Desenvolvedor -> String
- Descricao -> String
- Classificacao_Etaria -> String

Usuario:
- ID -> Integer
- Usuario -> String
- Senha -> String
- Administrador_Flag -> Boolean
- Data_Nascimento -> LocalDate

Nota:
- ID -> Integer
- ID_Usuario -> Integer
- ID_Jogo -> Integer
- Nota_Trilha_Sonora -> Integer
- Nota_Graficos -> Integer
- Nota_Historia -> Integer
- Nota_Jogabilidade -> Integer
- Comentario -> String
- Data_Nota -> LocalDate

Genero:
- ID -> Integer
- Genero -> String

JogoGenero:
- ID -> Integer
- ID_Jogo -> Integer
- ID_Genero -> Integer

UsuarioGenero:
- ID -> Integer
- ID_Usuario -> Integer
- ID_Genero -> Integer
